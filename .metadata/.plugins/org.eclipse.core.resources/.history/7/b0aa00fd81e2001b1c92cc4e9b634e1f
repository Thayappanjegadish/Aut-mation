package Democlasses;

import org.testng.annotations.BeforeTest;
import org.testng.annotations.Test;


public class Demoone {
	
	String name = "Jegadish";
	String secondname = "Thayappan";
	
	
	int a = 7;
	int b = 7;
	
	
	@Test(groups= {"Smoke"})
	public void caseone() {
		System.out.println(name + secondname + "------Two characters has been concatenated successfully-----");
	}
	
	@Test
	public void casetwo() {
		int c = a + b;
		System.out.println(c);
	}
	
	
	
	

}
