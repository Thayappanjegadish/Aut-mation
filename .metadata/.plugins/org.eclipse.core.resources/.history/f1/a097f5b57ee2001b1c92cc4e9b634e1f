		System.out.println(urlname);
	}
	
	@Test(groups= {"Smoke"})
	public void webtwo() {
		System.out.println("Logged into webtwo");
	}
	
	@Test
	public void APIlogin() {
		System.out.println("Verified login API using postman");
	}

}
